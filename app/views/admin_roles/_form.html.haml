= form_for(@admin_role) do |f|
  - if @admin_role.errors.any?
    #error_explanation
      %h2
        = pluralize(@admin_role.errors.count, "error")
        prohibited this admin_role from being saved:
      %ul
        - @admin_role.errors.full_messages.each do |message|
          %li= message
  .field
    = f.label :user_id
    %br/
    = f.number_field :user_id
  .field
    = f.label :admin_type
    %br/
    = f.text_field :admin_type
  .field
    = f.label :admin_id
    %br/
    = f.number_field :admin_id
  .actions
    = f.submit
